<?xml version="1.0" encoding="utf-8" ?>
<skin>
  <layout>
    <CustomWindow id="chatdlg" width="653" height="420" minwidth="450" minheight="400"
                 transparent.type="layered" istransparent="1" xtransparent.maskalpha="200"
                 bkgnd.render.type="null"
                 font="normal">
      
      <Panel id="right_panel" layout.right="0" width="215" layout.top="0" layout.bottom="0"
           bkgnd.render.type="image"
           bkgnd.render.image="rightbkg.png"
           bkgnd.render.image.drawtype="stretch"
           bkgnd.render.image.9region="1,20,20,20"
             padding="10,20,10,20"
             >
        <QQContactMgrListView
              id="listview"
              layout.left="0"
              layout.top="0"
              layout.right="0"
              layout.bottom="0"
              istransparent="1"
              
              headerctrl.splitline.render.type="image"
              headerctrl.splitline.render.image="headsplitline"
              headerctrl.splitline.render.image.drawtype="center"
              headerctrl.bkgnd.render.type="imagelist"
              headerctrl.bkgnd.render.image="listhead.png"
              headerctrl.bkgnd.render.image.drawtype="stretch"
              
              bkgnd.render.type="image"
              bkgnd.render.image="list_bkg_border_.png"
              bkgnd.render.image.drawtype="stretch"
              bkgnd.render.image.9region="2"
              />
      </Panel>
      <Panel id="panel_left" layout.left="0" layout.right="209" layout.top="0" layout.bottom="0"
                  material.render.region="6,7,6,6"
                foregnd.render.type="image"
               foregnd.render.image="chatframe.png"
               foregnd.render.image.drawtype="stretch"
               foregnd.render.image.9region="12,101,12,10"
             >
        <Panel id="panel_header" border="6,6,6,0" layout.left="0" layout.top="0" layout.right="0" height="101"
               xforegnd.render.type="image"
               foregnd.render.image="chatframe.png"
               foregnd.render.image.drawtype="stretch"
               foregnd.render.image.9region="12,101,12,0"
               foregnd.render.image.srcregion="0,0,50,101"
               >
          <Label text="线程 'Win32 线程' (0xe7c) 已退出，返回值为 0 (0x0)。" layout.left="10" layout.top="78" textrender.color="white"/>
          <Panel layout="stack" layout.right="2" layout.top="2" layout.direction="righttoleft">
            <Button id="sys_close" autosize="bkimage" drawfocus="0" istransparent="1" tooltip="关闭"
                        bkgnd.render.type="imagelist" bkgnd.render.image="icon_close"></Button>
            <Button id="sys_maximize" autosize="bkimage" drawfocus="0" istransparent="1" tooltip="最大化"
                        bkgnd.render.type="imagelist" bkgnd.render.image="icon_max"></Button>
            <Button id="sys_restore" autosize="bkimage" drawfocus="0" istransparent="1" tooltip="还原"
                        bkgnd.render.type="imagelist"  bkgnd.render.image="icon_restore"></Button>
            <Button id="sys_minimize" autosize="bkimage" drawfocus="0" istransparent="1"  tooltip="最小化"
                        bkgnd.render.type="imagelist"  bkgnd.render.image="icon_min"></Button>
            <Button id="btn_expand_right_panel" tabable="1" autosize="bkimage" drawfocus="0" istransparent="1"  tooltip="右侧面板扩展/收缩"
                        bkgnd.render.type="imagelist" bkgnd.render.image="icon_min"></Button>
          </Panel>
        </Panel>

        
        <Panel id="panel_content" xlayout="split" xborder="6,0,6,0" layout.direction="v" istransparent="0"
               layout.left="6" layout.right="6" layout.top="101" layout.bottom="30"
               bkgnd.render.type="color"
               bkgnd.render.color="white"
               xforegnd.render.type="image"
               foregnd.render.image="chatframe.png"
               foregnd.render.image.drawtype="stretch"
               foregnd.render.image.9region="8"
               foregnd.render.image.srcregion="0,102,50,125"      
               
               layout.dragbar.bkgnd.render.type="image" layout.dragbar.bkgnd.render.image="chatframe_surface"
               layout.dragbar.height="20" layout.dragbar.width="2"
               layout.dragbarpos="100" layout.align="bottom">
          <xEdit id="testedit" layout.left="10" layout.top="5" width="200" height="22" bkgnd.render.type="null" bkgnd.render.color="white" istransparent="1"/>
          <RichEdit id="outputedit" istransparent="1" layout.left="10" layout.right="10" layout.top="0" layout.bottom="70" bkgnd.render.type="color" bkgnd.render.color="white" />
          <RichEdit id="inputedit" istransparent="1"  layout.left="10" layout.right="10" layout.bottom="10" height="50" bkgnd.render.type="color" bkgnd.render.color="white" layout.minsize="100"/>
          <SplitterBar layout.left="10" layout.bottom="60" layout.right="10" height="10"
                       top.id="outputedit" bottom.id="inputedit" top.minsize="30" bottom.minsize="30"
                       bkgnd.render.type="color" bkgnd.render.color="black"/>
        </Panel>

        <Panel id="panel_footer" layout.left="0" layout.bottom="0" layout.right="0" height="30"
               xforegnd.render.type="image"
               foregnd.render.image="chatframe.png"
               foregnd.render.image.drawtype="stretch"
               foregnd.render.image.9region="12"
               foregnd.render.image.srcregion="0,101,50,137"
               >          
        </Panel>
        
      </Panel>
    </CustomWindow>

  </layout>
</skin>
