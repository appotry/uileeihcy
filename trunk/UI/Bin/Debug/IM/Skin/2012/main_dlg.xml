<?xml version="1.0" encoding="utf-8"?>
<skin>
	<layout>
		<CustomWindow id="maindlg" width="290" height="540" minwidth="290" minheight="540" xmaxwidth="553" text="QQ 2013 V0.9.350" layout.right="30" bkgnd.render.type="image" bkgnd.render.image.drawtype="stretch" bkgnd.render.image="shadow_bkg.png" bkgnd.render.image.9region="10" material.render.region="6" istransparent="1" font="normal" transparent.type="areo" transparent.part="" transparent.part.9region="5">
