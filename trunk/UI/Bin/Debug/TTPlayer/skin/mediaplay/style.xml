<?xml version="1.0" encoding="utf-8" ?>
<skin>
  <style>
    <CustomWindow font="normal" transparentrgn.type="4corner" transparentrgn.9region="5"/>
    
    <class id="imagebtn" cursor="idc_hand" istransparent="1" autosize="bkimage" drawfocus="0" bkgnd.render.type="imagelist" bkgnd.render.imagelist.alphaanimate="1"/>
    
    <class id="imagecheckbutton"  cursor="idc_hand" autosize="content" foregnd.render.type="imagelist" drawfocus="0"  foregnd.render.imagelist.alphaanimate="1"/>
    
    <SliderCtrl 
              istransparent="1" 
              bkgnd.render.type="null" 
              foregnd.render.type="image" 
              cursor="idc_hand" 
              foregnd.render.image.drawtype="stretch"
              trackbtn.bkgnd.render.type="imagelist" 
              trackbtn.styleclass="imagebtn"/>

    <PictureCtrl foregnd.render.type="image"/>

    <SystemHScrollBar
             bkgnd.render.type="image" 
             bkgnd.render.image="hscrollbar_bar.bmp"
             bkgnd.render.image.9region="1"
             bkgnd.render.image.drawtype="stretch"
             linebtn1.bkgnd.render.type="imagelist"     
             linebtn1.bkgnd.render.image="hscrollbar_button_lineleft.bmp"
             linebtn1.styleclass="imagebtn"
             linebtn1.cursor="idc_arrow"
             linebtn2.bkgnd.render.type="imagelist"     
             linebtn2.bkgnd.render.image="hscrollbar_button_lineright.bmp"
             linebtn2.styleclass="imagebtn"
             linebtn2.cursor="idc_arrow"
             thumbbtn.styleclass="imagebtn"      
             thumbbtn.bkgnd.render.image="hscrollbar_thumb.bmp"
             thumbbtn.cursor="idc_arrow"        
             thumbbtn.bkgnd.render.type="imagelist"
             thumbbtn.bkgnd.render.image.drawtype="stretch"
             thumbbtn.bkgnd.render.image.9region="5"
             />
    <SystemVScrollBar  
             bkgnd.render.type="image" 
             bkgnd.render.image="scrollbar_bar.bmp"
             bkgnd.render.image.9region="1"
             bkgnd.render.image.drawtype="stretch"
             linebtn1.bkgnd.render.type="imagelist"     
             linebtn1.bkgnd.render.image="scrollbar_button_lineup.bmp"
             linebtn1.styleclass="imagebtn"
             linebtn1.cursor="idc_arrow"
             linebtn2.bkgnd.render.type="imagelist"     
             linebtn2.bkgnd.render.image="scrollbar_button_linedown.bmp"
             linebtn2.styleclass="imagebtn"
             linebtn2.cursor="idc_arrow"
             thumbbtn.bkgnd.render.type="imagelist"
             thumbbtn.bkgnd.render.image.drawtype="stretch"
             thumbbtn.bkgnd.render.image.9region="4"
             thumbbtn.styleclass="imagebtn"      
             thumbbtn.bkgnd.render.image="scrollbar_thumb.bmp"
             thumbbtn.cursor="idc_arrow"
             />

    <Menu    border="2,2,2,2"
             bkgnd.render.type="image"
             bkgnd.render.image="menu_bk.png"
             bkgnd.render.image.9region="24,2,2,2"
             bkgnd.render.image.drawtype="stretch"
             foregnd.render.type="colorlist"
             foregnd.render.color=";menu_item_hover;menu_item_hover;"
             foregnd.render.border.color=";menu_item_hover_border;menu_item_hover_border;"
             foregnd.render.border="1"
             foregnd.render.colorlist.count="4"
             icongutterwidth="22"
             textrender.type="colorlist"
             textrender.color="black;black;black;gray"
             textleftindent="5"
             textrightindent="10"
             separatorheight="8"
             radioicon.render.type="image"
             radioicon.render.image="menu_check_icon.png"
             radioicon.render.image.drawtype="center"
             checkicon.render.type="image"
             checkicon.render.image="menu_check_icon.png"
             checkicon.render.image.drawtype="center"
             radiocheckiconbk.render.type="image"
             radiocheckiconbk.render.image="menu_check_bk.png"
             radiocheckiconbk.render.image.drawtype="center"
             xtransparent.type="layered" 
             xtransparent.maskalpha="250"
             ></Menu>

    <class   id="mainmenu"  
             border="24,3,2,3" 
             bkgnd.render.image="main_menu_bk.png"
             bkgnd.render.image.9region="50,5,5,135"/>
    <class   id="mainmenuitem"  
             icon.render.type="imagelistitem" 
             icon.render.image="menuicon_imagelist.png" 
             icon.render.image.drawtype="adapt"/>
    <class   id="playmodemenuitem"  
             icon.render.type="imagelistitem" 
             icon.render.image="playmode_imagelist.png" 
             icon.render.image.drawtype="adapt"/>
  </style>
</skin>
