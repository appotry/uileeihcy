<?xml version="1.0" encoding="utf-8" ?>

<skin>
  <layout>
    
    <Window id="ProjectTreeDialog">
      <TabCtrl id="tabctrl" padding="10" layout.left="0" layout.top="0" layout.right="0" layout.bottom="0" head.layout="average">
        <item>
          <head>
            <TabCtrlButton text="layout"/>
          </head>
          <content>
            <TreeView id="treelayout"/>
          </content>
        </item>

        <xitem>
          <head>
            <TabCtrlButton text="xml doc"/>
          </head>
        </xitem>

        <item>
          <head>
            <TabCtrlButton text="resource"/>
          </head>
          <content>
            <TreeView id="treeresource"/>
          </content>
        </item>

      </TabCtrl>
    </Window>

  </layout>
</skin>