<?xml version="1.0" encoding="utf-8" ?>
<skin>

<log enable="1">
  <item name="ui" enable="1" path="log\ui.log" level="debug" flag="dbgview"  fileflag="disable"/>
</log>
  
<layout>
  <Window id="mainwindow" minwidth="200" minheight="200">
    <xListBox id="test_listbox" layout.left="10" layout.top="10" layout.right="10" layout.bottom="10"/>
    <xListView id="test_listview" layout.left="10" layout.top="10" width="300" height="300"/>
    <xComboBox id="combo" readonly="1" layout.left="400" layout.top="30" width="150" height="25"/>
    <xScrollPanel id="scrollpanel1" layout.left="0"  layout.top="0" layout.right="0" layout.bottom="0"
        viewsize="2000,2000" vscrollbar.type="SystemVScrollBar" hscrollbar.type="SystemHScrollBar">
      <PropertyCtrl id="test_propertyctrl" layout.left="10" layout.top="10" width="300" height="300"/>
      <xEdit layout.left ="100" layout.top="100" width="300" height="30"/>
    </xScrollPanel>
    <Panel layout="stack" layout.direction="toptobottom">
      <Panel layout="stack">
        <Button width="80" height="30"/>
        <Button width="80" height="30"/>
        <Button width="80" height="30"/>
        <Button width="80" height="30"/>
        <Button width="80" height="30"/>
      </Panel>
      <Panel layout="stack">
        <Button width="80" height="30"/>
        <Button width="80" height="30"/>
        <Button width="80" height="30"/>
        <Button width="80" height="30"/>
        <Button width="80" height="30"/>
      </Panel>
      <Panel layout="stack">
        <Button width="80" height="30"/>
        <Button width="80" height="30"/>
        <Button width="80" height="30"/>
        <Button width="80" height="30"/>
        <Button width="80" height="30"/>
      </Panel>
      <Panel layout="stack">
        <Button width="80" height="30"/>
        <Button width="80" height="30"/>
        <Button width="80" height="30" defpushbutton="1"/>
        <Button width="80" height="30"/>
        <Button width="80" height="30" defpushbutton="0"/>
      </Panel>
      <Panel>
        <Edit width="200" height="30"/>
      </Panel>
    </Panel>
  </Window>
</layout>
  
<image>
</image>
  
<font>
   <item id="normal" facename="微软雅黑" size="18" bold="0"/>
   <item id="normal2" facename="宋体" size="9"/>
</font>
  
<color>
  <item id="border">0,0,0,255</item>
  <item id="dlgbk">255,255,255,255</item>
